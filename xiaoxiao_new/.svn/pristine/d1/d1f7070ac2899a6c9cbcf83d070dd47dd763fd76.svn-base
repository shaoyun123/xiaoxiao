package com.web.model;

import java.text.SimpleDateFormat;
import java.util.Date;

public class NeiRongTongZhi {
	
	private int id;
	
	private String title;
	
	private String content;
	
	private int faburenid;
	
	private int faburenleixing;
	
	private int fabufanwei;
	
	private String tiaojian;
	
	private boolean isTop;
	
	private Date faburiqi;
	
	private String faburenxingming;
	
	private String url;
	

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getFaburenxingming() {
		return faburenxingming;
	}

	public void setFaburenxingming(String faburenxingming) {
		this.faburenxingming = faburenxingming;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public int getFaburenid() {
		return faburenid;
	}

	public void setFaburenid(int faburenid) {
		this.faburenid = faburenid;
	}

	public int getFaburenleixing() {
		return faburenleixing;
	}

	public void setFaburenleixing(int faburenleixing) {
		this.faburenleixing = faburenleixing;
	}

	public int getFabufanwei() {
		return fabufanwei;
	}

	public void setFabufanwei(int fabufanwei) {
		this.fabufanwei = fabufanwei;
	}

	public String getTiaojian() {
		return tiaojian;
	}

	public void setTiaojian(String tiaojian) {
		this.tiaojian = tiaojian;
	}

	public boolean getIstop() {
		return isTop;
	}

	public void setIstop(boolean isTop) {
		this.isTop = isTop;
	}

	public String getFaburiqi() {
		SimpleDateFormat sdf = new SimpleDateFormat("yyyy-MM-dd");
    	return sdf.format(faburiqi);
	}

	public void setFaburiqi(Date faburiqi) {
		this.faburiqi = faburiqi;
	}
	
	

}
